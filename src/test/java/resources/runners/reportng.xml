<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Test suite" verbose="1" >
    <listeners>
        <listener class-name="com.tutorialsninja.customlisteners.CustomListeners"/>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
    </listeners>
    <test name="reportng" >
        <classes>
            <class name="com.tutorialsninja.testsuite.DesktopsTest"></class>
            <class name="com.tutorialsninja.testsuite.LaptopsAndNotebooksTest"></class>
            <class name="com.tutorialsninja.testsuite.MyAccountsTest"></class>
            <class name="com.tutorialsninja.testsuite.TopMenuTest"></class>
        </classes>
    </test>
</suite>
